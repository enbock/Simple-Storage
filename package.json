{
  "name": "@enbock/simple-storage",
  "description": "A simple storage for Progressive Web Applications.",
  "version": "2.0.0",
  "homepage": "https://github.com/enbock/Simple-Storage/",
  "license": "MIT",
  "author": "Endre Bock <dev@itbock.de>",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npx tsc --build tsconfig.json -v",
    "test": "jest --coverage --verbose"
  },
  "dependencies": {
    "@enbock/state-value-observer": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-typescript": "^7.10.1",
    "@babel/runtime": "^7.10.3",
    "@types/jest": "^26.0.3",
    "jest": "^26.1.0",
    "typescript": "^3.9.5"
  },
  "jest": {
    "roots": [
      "./src"
    ],
    "collectCoverageFrom": [
      "**/*.ts"
    ],
    "testEnvironment": "node",
    "testRegex": "\\.test\\.ts$",
    "testURL": "http://localhost",
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100
      }
    }
  }
}
